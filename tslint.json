{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended", "tslint-eslint-rules", "tslint-no-circular-imports"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "ter-indent": [true, 4],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "interface-name" : [true, "never-prefix"],
    "member-access": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}
